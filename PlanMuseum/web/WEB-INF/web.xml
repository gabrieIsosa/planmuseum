<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MuseoImagen</servlet-name>
        <servlet-class>paquetemuseo.MuseoImagen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarMuseo</servlet-name>
        <servlet-class>paquetemuseo.EditarMuseo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarMuseoServlet</servlet-name>
        <servlet-class>paquetemuseo.EditarMuseoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanosImagen</servlet-name>
        <servlet-class>paquetemuseo.PlanosImagen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarPlanoServlet</servlet-name>
        <servlet-class>paquetemuseo.EditarPlanoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarMuseo</servlet-name>
        <servlet-class>paquetemuseo.EliminarMuseo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminacionMuseo</servlet-name>
        <servlet-class>paquetemuseo.EliminacionMuseo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImagenServlet</servlet-name>
        <servlet-class>paquetemuseo.ImagenServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarImgServlet</servlet-name>
        <servlet-class>paquetemuseo.EditarImgServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MuseoImagen</servlet-name>
        <url-pattern>/MuseoImagen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarMuseo</servlet-name>
        <url-pattern>/EditarMuseo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarMuseoServlet</servlet-name>
        <url-pattern>/EditarMuseoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanosImagen</servlet-name>
        <url-pattern>/PlanosImagen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarPlanoServlet</servlet-name>
        <url-pattern>/EditarPlanoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarMuseo</servlet-name>
        <url-pattern>/EliminarMuseo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminacionMuseo</servlet-name>
        <url-pattern>/EliminacionMuseo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImagenServlet</servlet-name>
        <url-pattern>/ImagenServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarImgServlet</servlet-name>
        <url-pattern>/EditarImgServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
